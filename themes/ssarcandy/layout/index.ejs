<div class="post-list">
  <% page.posts.each(function(post) { %>
    <div class="post-list-item">
        <%- partial('_partial/index-item', {
            post: post, 
            index: true
        }) %>
    </div>
  <% }) %>
</div>
<% if (page.total > 1) { %>
<nav id="page-nav">
    <div class="inner">
    <%- paginator({
        prev_text: '上一頁',
        next_text: '下一頁'
    }) %>
    </div>
</nav>
<% } %>